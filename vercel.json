{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "ignoreCommand": "bash -c 'if [ \"$VERCEL_GIT_COMMIT_REF\" = \"gh-pages\" ]; then exit 0; else exit 1; fi'",
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  },
  "env": {
    "PUBLIC_URL": ""
  },
  "rewrites": [
    {
      "source": "/api/getsongbpm/(.*)",
      "destination": "/api/getsongbpm/$1"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" }
      ]
    }
  ]
}
